<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zfy.yuio.dao.SysDictDataDao">
    <insert id="add" parameterType="SysDictData">
        INSERT INTO sys_dict_data(dict_id,dict_label,dict_type,dict_value,status,remark)
        VALUES (#{dictId},#{dictLabel},#{dictType},#{dictValue},#{status},#{remark})
    </insert>

    <select id="get" resultType="SysDictData" parameterType="String">
        SELECT * FROM sys_dict_data
        WHERE dict_type=#{dictType}
    </select>

    <delete id="del" parameterType="String">
        DELETE FROM sys_dict_data
        WHERE dict_id=#{id}
    </delete>

    <update id="upd" parameterType="SysDictData">
        UPDATE sys_dict_data
        SET dict_label=#{dictLabel},dict_value=#{dictValue},status=#{status},remark=#{remark}
        WHERE dict_id=#{dictId}
    </update>
</mapper>