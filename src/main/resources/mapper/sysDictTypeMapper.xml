<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zfy.yuio.dao.SysDictTypeDao">
    <insert id="add" parameterType="SysDictType">
        INSERT INTO sys_dict_type(dict_id,dict_name,dict_type,status,remark)
        VALUES (#{dictId},#{dictName},#{dictType},#{status},#{remark})
    </insert>

    <select id="get" resultType="SysDictType">
        SELECT * FROM sys_dict_type
    </select>

    <delete id="del" parameterType="String">
        DELETE FROM sys_dict_type
        WHERE dict_id=#{id}
    </delete>

    <update id="upd" parameterType="SysDictType">
        UPDATE sys_dict_type
        SET dict_name=#{dictName},dict_type=#{dictType},status=#{status},remark=#{remark}
        WHERE dict_id=#{dictId}
    </update>
</mapper>